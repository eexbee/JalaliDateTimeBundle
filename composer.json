{
    "name": "eexbee/jalali-date-time-bundle",
    "description": "Brings Jalali(Persian/Iranian) DateTime to Symfony/Twig",
    "type": "symfony-flex",
    "license": "MIT",
    "keywords": ["datetime", "jalali", "persian"],
    "homepage": "https://github.com/eexbee/JalaliDateTimeBundle",
    "authors": [
        {
            "name": "Amin Mosayebi",
            "email": "me@eexbee.com"
        }
    ],
    "require": {
        "php": "^7.1.3",
        "symfony/config": "^3.4 || ^4.4 || ^5.0",
        "symfony/dependency-injection": "^3.4 || ^4.4 || ^5.0",
        "symfony/http-kernel": "^3.4 || ^4.4 || ^5.0"
    },
    "require-dev": {
        "symfony/framework-bundle": "^3.4 || ^4.4 || ^5.0",
        "symfony/phpunit-bridge": "^3.4 || ^4.4 || ^5.0"
    },
    "autoload": {
        "psr-4": {
            "Eexbee\\JalaliDateTimeBundle\\": ""
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Eexbee\\JalaliDateTimeBundle\\Tests\\": "Tests/"
        }
    }
}
